<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

	<table name="t_dept" schema="dbo" catalog="db_emp"
		class="com.qhit.lh.gr3.ming.bean.Dept">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="t_emp" schema="dbo" catalog="db_emp"
		class="com.qhit.lh.gr3.ming.bean.Emp">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
<table name="t_user" schema="dbo" catalog="db_emp"
	class="com.qhit.lh.gr3.ming.bean.User">
	<primary-key>
		<generator class="foreign"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>