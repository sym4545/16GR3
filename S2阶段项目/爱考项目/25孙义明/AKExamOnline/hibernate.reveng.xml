<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="t_student" schema="dbo" catalog="db_exam"
		class="com.qhit.lh.grs.wjk.exam.common.bean.Student">
		<primary-key>
			<generator class="foreign"></generator>
		</primary-key>
	</table>
	<table name="t_teacher" schema="dbo" catalog="db_exam"
		class="com.qhit.lh.grs.wjk.exam.common.bean.Teacher">
		<primary-key>
			<generator class="foreign"></generator>
		</primary-key>
	</table>
	<table name="t_user" schema="dbo" catalog="db_exam"
		class="com.qhit.lh.grs.wjk.exam.common.bean.User">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="t_course" schema="dbo" catalog="db_exam"
		class="com.qhit.lh.grs.wjk.exam.kmgl.bean.Course">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="t_machineTest" schema="dbo" catalog="db_exam"
		class="com.qhit.lh.grs.wjk.exam.tkgl.bean.MachineTest">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="t_writtenTest" schema="dbo" catalog="db_exam"
		class="com.qhit.lh.grs.wjk.exam.tkgl.bean.WrittenTest">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="t_classInfo" schema="dbo" catalog="db_exam"
		class="com.qhit.lh.grs.wjk.exam.bjgl.bean.ClassInfo">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="paper_class" schema="dbo" catalog="db_exam"
		class="com.qhit.lh.grs.wjk.exam.sjgl.bean.PaperClass">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
<table name="t_paper" schema="dbo" catalog="db_exam"
	class="com.qhit.lh.grs.wjk.exam.sjgl.bean.Paper">
	<primary-key>
		<generator class="native"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>